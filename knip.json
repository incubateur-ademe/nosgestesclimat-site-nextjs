{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "tags": ["-lintignore"],
  "entry": [
    "sentry.*.config.ts",
    ".storybook/**/*.{ts,tsx,js,jsx}",
    "scripts/**/*.{ts,tsx,js,jsx,mjs,cjs}",
    "src/i18nConfig.ts",
    "public/scripts/**/*.{ts,tsx,js,jsx}",
    "check-memory.mjs"
  ],
  "ignore": [
    "**/*.test.{ts,tsx,js,jsx}",
    "**/*.spec.{ts,tsx,js,jsx}",
    "**/__tests__/**",
    "**/__mocks__/**",
    "tests/**",
    "src/helpers/tests/**",
    "**/*.stories.{ts,tsx,js,jsx}",
    "public/**",
    ".next/**",
    "node_modules/**",
    "vitest.setup.ts",
    "vitest-globals.d.ts",
    "src/locales/nouveautes/**",
    "scripts/**",
    "src/constants/ab-test.ts",
    "e2e/**",
    "src/locales/initClient.ts"
  ],
  "ignoreDependencies": [
    "@axe-core/playwright",
    "i18next-parser",
    "yaml-loader",
    "playwright-core",
    "react-tooltip",
    "eslint-plugin-react-hooks"
  ],
  "ignoreBinaries": ["only-allow"]
}
