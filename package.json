  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "dev": "next dev",
    "dev:webpack": "next dev --webpack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:debug-memory": "next build --experimental-debug-memory-usage",
    "format": "prettier --write \"**/*.{js,ts,tsx,md,json}\" && eslint . --fix",
    "start": "next start",
    "lint": "prettier --check --cache  . && eslint --cache --cache-location './node_modules/.cache/eslint' --quiet",
    "lint:fix": "eslint --cache --cache-location './node_modules/.cache/eslint' --fix",
    "lint:types": "tsc --noEmit",
    "e2e": "playwright test --ui",
    "iframe-healthcheck": "node scripts/healthchecks/iframes.js",
    "ui:parse": "node scripts/i18n/generate-ui.js",
    "ui:check": "pnpm run ui:parse && node scripts/i18n/check-ui.js",
    "ui:translate": "node scripts/i18n/translate-ui.js",
    "ui:clean": "node scripts/i18n/generate-ui.js -r",
    "faq:translate": "node scripts/i18n/translate-faq.js",
    "faq:check": "node scripts/i18n/check-faq.js",
    "pages:translate": "node scripts/i18n/translate-pages.cjs",
    "nouveautes:translate": "node scripts/i18n/translate-nouveautes.cjs",
    "ts:prune": "ts-prune",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:build:github-pages": "cp .storybook/main.github-pages.ts .storybook/main.ts && cp .storybook/preview.github-pages.ts .storybook/preview.ts && storybook build && git checkout .storybook/main.ts .storybook/preview.ts",
    "build-storybook": "storybook build",
    "scalingo-postbuild": "pnpm build && pnpm prune --prod"
  },
